{
  "name": "GraphqlTest",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://mr-bagglesworth.github.io/Github-GraphQL/",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest --silent",
    "test:watch": "jest --silent --watch",
    "test:coverage": "jest --silent --coverage",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.5.1",
    "apollo-client": "^2.5.1",
    "apollo-link-http": "^1.5.12",
    "base-64": "^0.1.0",
    "gh-pages": "^2.0.1",
    "graphql": "^14.1.1",
    "graphql-tag": "^2.10.1",
    "react": "next",
    "react-apollo": "^2.5.2",
    "react-dom": "next",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "jest": "23.6.0",
    "jest-dom": "^3.1.3",
    "react-scripts": "^2.1.8",
    "react-test-renderer": "^16.8.4",
    "react-testing-library": "^5.4.4",
    "waait": "^1.0.4"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
